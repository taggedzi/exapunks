NOTE 293, 42, 79

; XA
GRAB 300
MODE
MARK INIT
  COPY F X
  REPL SEEK_INIT
  JUMP INIT

MARK SEEK_INIT
  LINK 800
  SWIZ X 3 T
MARK SEEK
  LINK 800 
  SUBI T 1 T 
  TJMP SEEK
  NOTE FOUND
  REPL WRITER_INIT
  SWIZ X 21 T
  ADDI 200 T T
  GRAB T
  COPY M X
MARK READING
  COPY F M
  TEST EOF
  FJMP READING
  KILL
  DROP
  GRAB X
MARK HOME
  @REP 10
  LINK -1
  @END

MARK WRITER_INIT
  MAKE
  FILE M
MARK WRITER
  COPY M F
  JUMP WRITER