NOTE 354, 43, 2

; XA
LINK 800
MAKE
COPY 0 F
COPY 0 F

MARK LOOP_READ
SEEK -9999
COPY F #DATA
COPY F #DATA
COPY M #DATA

MARK INCREMENT
SEEK -1
COPY F X
ADDI X 1 X
SEEK -1
COPY X F
TEST X > 8
TJMP BIGGER
JUMP LOOP_READ

MARK BIGGER
SEEK -2
COPY F X
ADDI X 1 X
SEEK -1
COPY X F
COPY 0 F
TEST X = 3
TJMP END
JUMP LOOP_READ

MARK END
WIPE
HALT

; XB
GRAB 300
MARK LOOP
TEST EOF
COPY F M
TJMP END
JUMP LOOP

MARK END
WIPE
HALT

; XC
LINK 800
COPY 0 #CLRS
HALT
